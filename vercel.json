{
  "version": 2,
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build", "config": {} }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
}
